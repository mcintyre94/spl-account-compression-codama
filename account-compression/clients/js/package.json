{
  "name": "@solana-program/spl-account-compression",
  "version": "1.0.0",
  "description": "JavaScript client for the SPL Account Compression program",
  "sideEffects": false,
  "module": "./dist/src/index.mjs",
  "main": "./dist/src/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js"
    }
  },
  "files": [
    "./dist/src",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && tsup && tsc -p ./tsconfig.declarations.json",
    "prepublishOnly": "pnpm build",
    "lint": "eslint --ext js,ts,tsx src",
    "lint:fix": "eslint --fix --ext js,ts,tsx src",
    "format": "prettier --check src",
    "format:fix": "prettier --write src"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solana-program/account-compression.git"
  },
  "bugs": {
    "url": "https://github.com/solana-program/account-compression/issues"
  },
  "homepage": "https://github.com/solana-program/account-compression#readme",
  "peerDependencies": {
    "@solana/kit": "^3.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "@solana/eslint-config-solana": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1"
  },
  "dependencies": {
    "@solana-program/token": "^0.6.0"
  }
}