{
  "name": "@mcintyre94/spl-account-compression",
  "version": "0.0.2",
  "description": "JavaScript client for the SPL Account Compression program",
  "sideEffects": false,
  "module": "./dist/src/index.mjs",
  "main": "./dist/src/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js"
    }
  },
  "files": [
    "./dist/src",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && tsup && tsc -p ./tsconfig.declarations.json",
    "prepublishOnly": "pnpm build",
    "test": "ava"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcintyre94/spl-account-compression.git"
  },
  "bugs": {
    "url": "https://github.com/mcintyre94/spl-account-compression/issues"
  },
  "homepage": "https://github.com/mcintyre94/spl-account-compression#readme",
  "peerDependencies": {
    "@solana/kit": "^2.1.1"
  },
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@types/node": "^24.1.0",
    "ava": "^6.4.1",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@solana-program/token": "^0.5.1"
  },
  "ava": {
    "nodeArguments": [
      "--no-warnings"
    ],
    "typescript": {
      "compile": false,
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  }
}